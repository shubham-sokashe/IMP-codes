<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name = "com.qickStart.demo.HIbernateFirstDemo.StudentPersistentClass" table = "hibernate_student_demo">
		<id name = "studentId" type = "int" column = "student_Id">
			<generator class = "assigned"/>
		</id>
		<property name = "studentName" column = "student_Name" type = "string"/>
		<property name = "studentAddress" column  = "student_Address" type = "string"/>
		<property name = "studentMarks" column = "student_Marks" type = "int"/>
		
	
	</class>
</hibernate-mapping>